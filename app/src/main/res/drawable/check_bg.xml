<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:height="26dp"
                android:width="26dp" />
            <solid android:color="@color/colorPrimaryDark" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <size
                android:width="26dp"
                android:height="26dp" />
            <solid android:color="@color/colorPrimary" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp"
        android:drawable="@drawable/ic_check"
        android:width="26dp"
        android:height="26dp" />
</layer-list>